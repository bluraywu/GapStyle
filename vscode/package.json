{
    "name": "gapstylevs",
    "displayName": "GapStyleVS",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "GapStyle VS",
                "uiTheme": "vs-dark",
                "path": "./themes/gapstyle.json"
            }
        ]
    },
    "scripts": {
        "dev": "nodemon -e yml --exec 'npm run build'",
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o ./bin/gapstyle.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "devDependencies": {
        "js-yaml": "^3.13.1",
        "nodemon": "^2.0.2",
        "tinycolor2": "^1.4.1"
    }
}
